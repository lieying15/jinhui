<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_water_card"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/app_mainback"
    android:fitsSystemWindows="true"
    tools:context="com.thlh.jhmjmw.business.user.coupon.ExchangeCardActivity">

    <com.thlh.jhmjmw.view.HeaderNormal
        android:id="@+id/exchange_card_headernormal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:title="兑换卡"/>

    <com.thlh.jhmjmw.view.ExchangeCardView
        android:id="@+id/exchange_card_noinfoview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <com.thlh.jhmjmw.view.NoInfoView
        android:id="@+id/exchange_card_infos"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>

</LinearLayout>
