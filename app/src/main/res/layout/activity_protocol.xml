<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows = "true"
    android:background="@color/white">
    <com.thlh.jhmjmw.view.HeaderNormal
        android:id="@+id/protocol_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        custom:title="充值换购预存协议"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="20dp"
                android:textSize="@dimen/text_size_small_x"
                android:textColor="@color/wine"
                android:text="充值换购预存协议"
                android:gravity="center"
                android:visibility="gone"/>
            <TextView
                android:id="@+id/protocol_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="12dp"
                android:layout_margin="18dp"
                android:textSize="@dimen/text_size_small_xx"
                android:textColor="@color/text_nomal"
                android:background="@drawable/shap_radius_white_stroke_mainback_r20"/>


        </LinearLayout>
    </ScrollView>
</LinearLayout>
